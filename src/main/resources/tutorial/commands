# Running the Code
mkdir compiled
javac src/main/java/ru/fintech/kerberos/tutorial/JaasAcn.java -d compiled
java -cp compiled -Djava.security.krb5.realm=HOPTO.ORG -Djava.security.krb5.kdc=kerbserver.hopto.org -Djava.security.auth.login.config=src/main/resources/tutorial/jaas.conf ru.fintech.kerberos.tutorial.JaasAcn

# Running the Code with a Security Manager
mkdir compiled
javac src/main/java/ru/fintech/kerberos/tutorial/JaasAcn.java -d compiled
jar -cvf compiled/JaasAcn.jar compiled/ru/fintech/kerberos/tutorial/JaasAcn.class
java -classpath JaasAcn.jar -Djava.security.manager -Djava.security.krb5.realm=HOPTO.ORG -Djava.security.krb5.kdc=kerbserver.hopto.org -Djava.security.policy=src/main/resources/tutorial/jaasacn.policy -Djava.security.auth.login.config=src/main/resources/tutorial/jaas.conf ru.fintech.kerberos.tutorial.JaasAcn